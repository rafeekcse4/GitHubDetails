<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    android:orientation="vertical"
    tools:context="githubdetail.com.githubdetails.Activity.DetailsActivity">

    <TextView
        android:id="@+id/id_txt_projectname_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Project  Name:"
        android:textColor="#353535"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/id_txt_projectname_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="akfndsknf"/>

    <TextView
        android:id="@+id/id_txt_ownername_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="User Name:"
        android:textColor="#353535"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/id_txt_ownername_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="akfndsknf"/>

    <TextView
        android:id="@+id/id_txt_owner_profile_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="User Profile Details:"
        android:textColor="#353535"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/id_txt_owner_profile_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="akfndsknf"/>

    <TextView
        android:id="@+id/id_txt_desc_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Description:"
        android:textColor="#353535"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/id_txt_desc_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="akfndsknf"/>

    <TextView
        android:id="@+id/id_txt_project_url_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Project Details:"
        android:textColor="#353535"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/id_txt_project_url_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="akfndsknf"/>

    <TextView
        android:id="@+id/id_txt_createdate_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Created Date:"
        android:textColor="#353535"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/id_txt_createdate_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="akfndsknf"/>





</LinearLayout>
